{
  "name": "@softtechai/quickmcp",
  "version": "1.0.16",
  "description": "An application to generate MCP servers from various data sources and test them",
  "main": "dist/index.js",
  "bin": {
    "quickmcp": "quickmcp-direct-stdio.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:dev": "DOTENV_CONFIG_PATH=.env.dev tsx src/index.ts",
    "dev:prod": "DOTENV_CONFIG_PATH=.env.prod tsx src/index.ts",
    "start:dev": "DOTENV_CONFIG_PATH=.env.dev node dist/index.js",
    "start:prod": "DOTENV_CONFIG_PATH=.env.prod node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test"
  },
  "files": [
    "dist",
    "quickmcp-direct-stdio.js",
    "src/web/public",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": "24.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/mssql": "^9.1.8",
    "@types/node": "^20.0.0",
    "basic-ftp": "^5.1.0",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.0",
    "imapflow": "^1.2.6",
    "mailparser": "^3.9.1",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "mongodb": "^6.8.0",
    "mysql2": "^3.6.0",
    "nodemailer": "^7.0.12",
    "pg": "^8.11.0",
    "sqlite3": "^5.0.2",
    "ssh2-sftp-client": "^12.0.1",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/multer": "^1.4.0",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server-generator",
    "ai",
    "llm"
  ],
  "author": "MCP Server Generator",
  "license": "MIT"
}
